  "description": "React version of the awesome json-editor https://github.com/json-editor/json-editor",
  "main": "dist/index.js",
  "types": "src/index.d.ts",
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
