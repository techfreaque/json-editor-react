    "@storybook/testing-library": "^0.0.13",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
